<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:FileEncryptorWpf.ViewModels"
                    xmlns:view="clr-namespace:FileEncryptorWpf.Views">
    <DataTemplate DataType="{x:Type vm:MainViewModel}">
        <view:MainControl />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:LoginViewModel}">
        <view:LoginControl />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:OutputViewModel}">
        <view:OutputControl />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:RegisterViewModel}">
        <view:RegisterControl />
    </DataTemplate>
</ResourceDictionary>